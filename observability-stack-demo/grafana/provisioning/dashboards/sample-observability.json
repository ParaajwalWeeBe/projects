{
  "annotations": {"list": []},
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "type": "timeseries",
      "title": "Request rate by status (req/s)",
      "datasource": {"type": "prometheus", "uid": null, "name": "Prometheus"},
      "targets": [
        {
          "expr": "sum(rate(app_requests_total[1m])) by (status)",
          "legendFormat": "status={{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "reqps"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "p95 latency (s)",
      "datasource": {"type": "prometheus", "uid": null, "name": "Prometheus"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(app_request_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "s"}, "overrides": []},
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "type": "logs",
      "title": "App logs (Loki)",
      "datasource": {"type": "loki", "uid": null, "name": "Loki"},
      "targets": [
        {
          "expr": "{container=\"app\"}",
          "refId": "A"
        }
      ],
      "options": {"showTime": true, "wrapLogMessage": true},
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8}
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["observability", "sample"],
  "timezone": "",
  "title": "Sample Observability",
  "version": 1,
  "weekStart": ""
}

